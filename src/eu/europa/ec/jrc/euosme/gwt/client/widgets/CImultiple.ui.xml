<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:client="urn:import:eu.europa.ec.jrc.euosme.gwt.client">	
	<g:VerticalPanel ui:field="myPanel">
		<g:DisclosurePanel ui:field="componentPanel">
	 		<g:customHeader>
	   			<g:HorizontalPanel>
   					<g:Label addStyleNames="h3" ui:field="myLabel"/>
   					<g:Button ui:field="addButton"/>
   					<client:InfoButton ui:field="infoButton"/>
	   			</g:HorizontalPanel>	   			     				
	   		</g:customHeader>
	 		<g:VerticalPanel ui:field="fieldsGroup"/>	
		</g:DisclosurePanel>
	</g:VerticalPanel>
</ui:UiBinder> 